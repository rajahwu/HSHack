{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "extensions": [
      "js",
      "jsx"
    ],
    "presets": [
      "@babel/preset-react"
    ]
  }
}